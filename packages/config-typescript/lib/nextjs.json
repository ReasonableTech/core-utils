{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["ES2023", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowJs": true,
    "jsx": "preserve",
    "jsxImportSource": "react",
    "composite": false,
    "declaration": false,
    "declarationMap": false,
    "noEmit": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
