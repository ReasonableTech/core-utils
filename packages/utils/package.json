{
  "name": "@reasonabletech/utils",
  "version": "0.1.1",
  "description": "Utility functions and helpers for TypeScript projects",
  "keywords": [
    "reasonabletech",
    "config",
    "typescript",
    "utilities",
    "helpers",
    "result-type",
    "async",
    "retry"
  ],
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "docs/**/*"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.js"
    },
    "./result": {
      "types": "./dist/src/result.d.ts",
      "import": "./dist/result.js"
    },
    "./datetime": {
      "types": "./dist/src/datetime.d.ts",
      "import": "./dist/datetime.js"
    },
    "./object": {
      "types": "./dist/src/object.d.ts",
      "import": "./dist/object.js"
    },
    "./string": {
      "types": "./dist/src/string.d.ts",
      "import": "./dist/string.js"
    },
    "./retry": {
      "types": "./dist/src/retry.d.ts",
      "import": "./dist/retry.js"
    }
  },
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly -p tsconfig.build.json",
    "clean": "rm -rf dist .turbo node_modules coverage tsconfig.tsbuildinfo",
    "dev": "tsup --watch",
    "docs": "tsx ../../../scripts/analysis/generate-api-docs.ts",
    "docs:api": "typedoc",
    "docs:coverage": "tsx ../../../scripts/analysis/check-doc-coverage.ts --html",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run tests/e2e",
    "test:integration": "vitest run tests/integration",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run tests/unit",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "verify:release": "pnpm typecheck && pnpm lint:check && pnpm test && pnpm build",
    "prepublishOnly": "pnpm verify:release"
  },
  "devDependencies": {
    "@reasonabletech/eslint-config": "workspace:*",
    "@reasonabletech/config-tsup": "workspace:*",
    "@reasonabletech/config-typescript": "workspace:*",
    "@reasonabletech/config-vitest": "workspace:*",
    "@types/node": "^25.3.1",
    "@vitest/coverage-v8": "4.0.18",
    "eslint": "10.0.2",
    "tsup": "8.5.1",
    "typedoc": "^0.28.17",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "license": "MIT",
  "author": "Reasonable Tech Company",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ReasonableTech/core-utils.git",
    "directory": "packages/utils"
  },
  "bugs": {
    "url": "https://github.com/ReasonableTech/core-utils/issues"
  },
  "homepage": "https://github.com/ReasonableTech/core-utils/tree/main/packages/utils",
  "engines": {
    "node": ">=22"
  }
}
